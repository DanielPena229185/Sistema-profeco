{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "default/exceptions/(.*)": "<rootDir>/../libs/exceptions/src/$1",
    "default/exceptions": "<rootDir>/../libs/exceptions/src"
  }
}